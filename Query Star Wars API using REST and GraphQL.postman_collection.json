							"mode": "graphql",
							"graphql": {
								"query": "{\n  allStarships {\n    edges {\n      node {\n        name\n        model\n        manufacturers\n        costInCredits\n        pilotConnection {\n          edges {\n            node {\n              name \n              homeworld {\n                name\n              }               \n            }\n          }\n        }\n      }\n    }\n  }\n}",
